	private static void startAC() 
	{
		int interactions = 0;
		Scanner scn = new Scanner(System.in);
		System.out.print("Quantas Interações: ");
		interactions = scn.nextInt();
		scn.close();
		
		int[][] acInicial = new int[interactions][100];
		Random r = new Random();
		for(int i=0;i<100;i++) {
			acInicial[0][i] = r.nextInt(1);
		}
		
	}
	
